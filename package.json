{
  "name": "bootstrap-ng2-spa-kickstart",
  "version": "1.0.0",
  "description": "Bootstrap and AngularJS 2 Single Page Application Kickstart",
  "scripts": {
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/devServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 8080",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.spec.ts\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/prodServer.js",
    "ts-build": "tsc --rootDir src --outDir dist",
    "postinstall": "typings install"
  },
  "author": "Jean Carlos de Toledo",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-latest": "6.22.0",
    "babel-register": "6.22.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "concurrently": "^3.2.0",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "eslint": "3.15.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "2.1.14",
    "express": "4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "9.11.0",
    "lite-server": "^2.2.2",
    "localtunnel": "1.8.2",
    "mocha": "3.2.0",
    "nock": "9.0.6",
    "node-sass": "^4.3.0",
    "npm-run-all": "4.0.1",
    "nsp": "2.6.2",
    "numeral": "2.0.4",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.0",
    "style-loader": "0.13.1",
    "ts-loader": "^2.0.0",
    "typescript": "~2.1.6",
    "typings": "^2.1.0",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-hot-middleware": "2.16.1",
    "webpack-md5-hash": "0.0.5"
  }
}
